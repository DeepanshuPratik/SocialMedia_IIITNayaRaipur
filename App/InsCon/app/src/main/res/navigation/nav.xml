<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.inscon.Home_fragment"
        android:label="Cureya"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_homeFragment_to_logInFragment"
            app:destination="@id/logInFragment" />
    </fragment>
    <fragment
        android:id="@+id/creatorfragment"
        android:name="com.example.inscon.Creators_fragment"
        android:label="About Us"
        tools:layout="@layout/fragment_creators" />
    <fragment
        android:id="@+id/Tsoc"
        android:name="com.example.inscon.clubs.Tsoc"
        android:label="Tsoc"
        tools:layout="@layout/fragment_tsoc" />
    <fragment
        android:id="@+id/TrainingnPlacement"
        android:name="com.example.inscon.clubs.TrainingnPlacement"
        android:label="TrainingnPlacement"
        tools:layout="@layout/fragment_trainingn_placement" />

    <fragment
        android:id="@+id/logInFragment"
        android:name="com.example.inscon.Login"
        android:label="fragment_log_in"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_logInFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>